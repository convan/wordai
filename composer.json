{
  "name": "convan/wordai",
  "type": "library",
  "description": "Small php wrapper for Wordai.com service using Laravel and Symfony frameworks.",
  "keywords": ["wordai", "synnonymizer"],
  "homepage": "https://github.com/convan/wordai",
  "license": "MIT",
  "minimum-stability": "stable",
  "prefer-stable": true,
  "authors": [
    {
      "name": "Con Van",
      "email": "iv@reclarium.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Wordai\\": "src/Wordai"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Wordai\\Tests\\": "tests"
    }
  },
  "require": {
    "php": "^7.0 || ^7.1"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.7"
  }
}
